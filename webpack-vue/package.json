{
  "name": "icarus",
  "version": "1.0.0",
  "private": true,
  "description": "icarus - project template",
  "scripts": {
    "build": "cross-env NODE_ENV=development webpack --config ./webpack.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack.config.js",
    "prod": "cross-env NODE_ENV=production webpack --config ./webpack.config.js",
    "check": "npm run check:eslint && npm run check:prettier && npm run check:style --fix",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:style --fix",
    "check:eslint": "eslint --ext .js,.jsx,.ts,.tsx,.vue,.html src/",
    "lint:eslint": "eslint --fix --ext .js,.jsx,.ts,.tsx,.vue,.html src/",
    "check:prettier": "prettier --check 'src/**/*.{js,jsx,ts,tsx,vue,html}'",
    "lint:prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,vue,html}'",
    "check:style": "stylelint **/*.{css,less,sass,scss}",
    "lint:style": "stylelint --fix **/*.{css,less,sass,scss}",
    "prepare": "husky install",
    "cz": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@element-plus/icons-vue": "^2.1.0",
    "@soda/friendly-errors-webpack-plugin": "^1.8.1",
    "@types/js-cookie": "^3.0.3",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.2",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.40.0",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.5",
    "commitizen": "^4.2.5",
    "commitlint-config-cz": "^0.13.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.1",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.6.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.0",
    "js-cookie": "^3.0.1",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.18",
    "postcss-import": "^15.0.0",
    "postcss-loader": "^7.0.1",
    "postcss-scss": "^4.0.5",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.13.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "svg-sprite-loader": "^6.0.11",
    "tailwindcss": "^3.2.4",
    "typescript": "*",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.8",
    "vue-eslint-parser": "^9.1.0",
    "vue-loader": "^17.0.0",
    "vue-template-compiler": "^2.7.10",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "core-js": "^3.4.4",
    "element-plus": "2.2.28",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.23",
    "vue": "^3.2.36",
    "vue-router": "^4.1.5"
  }
}
